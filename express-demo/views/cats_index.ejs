<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Super Cool Cats App!!!</title>
  </head>
  <body>
    <h1>Cats</h1>

    <a href="/cats/new">New Cat</a>

    <ul>
      <% cats.forEach(cat => { %>
      <li>
        <a href="/cats/<%= cat.id %>">
          <%= cat.name %>
        </a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
